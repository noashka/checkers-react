{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "ES2018",
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "preserveConstEnums": true,
    "removeComments": false,
    "lib": ["es7", "dom"],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true
  }
}
